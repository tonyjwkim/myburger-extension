{
  "manifest_version": 3,
  "name": "myburger",
  "version": "1.0",
  "description": "Elevate your browsing experience with our intuitive Chrome extension",
  "permissions": [
    "activeTab",
    "storage",
    "identity"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/burger.png",
      "48": "icons/burger.png",
      "128": "icons/burger.png"
    }
  },
  "icons": {
    "16": "icons/burger.png",
    "48": "icons/burger.png",
    "128": "icons/burger.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "oauth2": {
    "client_id": "1055267845953-2ko3t2o71vo97vo50441ghuvmf6jqhks.apps.googleusercontent.com",
    "scopes": [
      "openid",
      "email"
    ]
  }
}